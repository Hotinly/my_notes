<h1>== Home ==</h1>

<h2>Hello World</h2>
<p>
  The time is now: <%= Time.now %>
</p>
<pre>This is the app where I am tacking notes.</pre>
<div class="theme-showcase"> <!-- <div class="container theme-showcase"> -->
  <div id="carousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators hidden-xs">
      <li data-target="#carousel" data-slide-to="0" class=""></li>
      <li class="active" data-target="#carousel" data-slide-to="1"></li>
      <li class="" data-target="#carousel" data-slide-to="2"></li>
    </ol>
    <!-- http://www.atool.org/img2base64.php -->
    <!-- http://www.yyyweb.com/ctools/demo.php?t=http%3A%2F%2Feditor.method.ac%2F&h=2000&c=&n= -->
    <div class="carousel-inner" role="listbox">
      <div class="item">
        <img class="carousel-inner img-responsive img-rounded" data-holder-rendered="true" src="data:image/svg+xml;base64," alt="First slide [1280x300]">
      </div>
      <div class="item active">
        <img class="carousel-inner img-responsive img-rounded" data-holder-rendered="true" src="data:image/svg+xml;base64," alt="Second slide [1280x300]">
      </div>
      <div class="item">
        <img class="carousel-inner img-responsive img-rounded" data-holder-rendered="true" src="data:image/svg+xml;base64," alt="Third slide [1280x300]">
      </div>
    </div>
    <a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#carousel" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>

<script type="text/javascript">
  $('.carousel').carousel({
    interval: 3000 // in milliseconds
  });
  $('#carousel').hover(function () {
    $(this).carousel('pause')
  });
  // $(".item img").addClass("carousel-inner img-responsive img-rounded");

</script>


<div class="footer navbar-fixed-bottom">
  <div class="container">
    <center class="text-muted">Designed by Holy 2017</center>
  </div>
</div>
